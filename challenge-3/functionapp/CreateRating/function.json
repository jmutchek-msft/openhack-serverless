{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "name": "ratingsDocument",
      "type": "cosmosDB",
      "databaseName": "ratings",
      "collectionName": "ratings",
      "createIfNotExists": true,
      "connectionStringSetting": "MyAccount_COSMOSDB",
      "direction": "out"

    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ]
}
